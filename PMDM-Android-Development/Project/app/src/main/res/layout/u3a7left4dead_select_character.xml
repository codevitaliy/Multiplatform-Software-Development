<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#5A1E1E"
    tools:context=".u3.u3a7left4deadSelectCharacter">

    <TextView
        android:id="@+id/u3a7SelectCharacterTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/u3a7strSelectCharacter"
        android:textColor="@color/white"
        android:textSize="35sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.156" />

    <ImageButton
        android:id="@+id/u3a7btnCharacter1"
        android:layout_width="126dp"
        android:layout_height="138dp"
        android:adjustViewBounds="true"
        android:background="#5A1E1E"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.203"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.349"
        app:srcCompat="@drawable/character_1" />

    <ImageButton
        android:id="@+id/u3a7btnCharacter2"
        android:layout_width="126dp"
        android:layout_height="138dp"
        android:adjustViewBounds="true"
        android:background="#5A1E1E"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.852"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.349"
        app:srcCompat="@drawable/character_2" />

    <ImageButton
        android:id="@+id/u3a7btnCharacter3"
        android:layout_width="126dp"
        android:layout_height="138dp"
        android:adjustViewBounds="true"
        android:background="#5A1E1E"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.203"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.632"
        app:srcCompat="@drawable/character_3" />

    <ImageButton
        android:id="@+id/u3a7btnCharacter4"
        android:layout_width="126dp"
        android:layout_height="138dp"
        android:adjustViewBounds="true"
        android:background="#5A1E1E"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.852"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.632"
        app:srcCompat="@drawable/character_4" />

    <ImageButton
        android:id="@+id/u3a7btnGoBackCharacter"
        android:layout_width="132dp"
        android:layout_height="63dp"
        android:adjustViewBounds="true"
        android:background="#5A1E1E"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.211"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.899"
        app:srcCompat="@drawable/go_back_bullet" />

    <ImageButton
        android:id="@+id/u3a7btnClearCharacter"
        android:layout_width="132dp"
        android:layout_height="63dp"
        android:adjustViewBounds="true"
        android:background="#5A1E1E"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.851"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.895"
        app:srcCompat="@drawable/clean_bullet" />

</androidx.constraintlayout.widget.ConstraintLayout>